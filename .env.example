# CyborgMind V2.6 Environment Configuration Example
# Copy to .env and customize

# GPU Configuration
CUDA_VISIBLE_DEVICES=0

# Logging
LOG_LEVEL=INFO

# Model Storage
TORCH_HOME=/app/models

# Experiment Tracking
WANDB_MODE=offline
WANDB_PROJECT=cyborgmind-v2.6

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=1

# Training Configuration
MAX_EPISODES=1000
CHECKPOINT_INTERVAL=100
